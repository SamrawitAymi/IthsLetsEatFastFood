<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="LetsApi.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>LetsApi.json</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="LetsApi.json">
      <Generator>NSwagCodeGenerator</Generator>
      <LastGenOutput>LetsApi.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
